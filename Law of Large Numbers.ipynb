    "import numpy as np\n",
    "from numpy.random import randn\n",
    "\n",
    "n = 1000\n",
    "counter = 0\n",
    "for i in randn(n):\n",
    "    if (i > -1 and i < 1):\n",
    "        counter += 1\n",
    "\n",
    "print(counter/n)"
   ]
